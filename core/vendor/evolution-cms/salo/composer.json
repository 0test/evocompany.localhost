{
    "name": "evolution-cms/salo",
    "description": "Docker files for running a basic evo application.",
    "keywords": ["evo", "docker"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "bin": [
        "bin/salo"
    ],
    "autoload": {
        "psr-4": {
            "EvolutionCMS\\Salo\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "EvolutionCMS\\Salo\\SaloServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
